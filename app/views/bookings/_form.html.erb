<h1>New Booking</h1>
<div class="w-50 mx-auto">

  <%= simple_form_for [pet, booking] do |f| %>
    <div class="form-inputs">
      <%= f.input :start_date %>
      <%= f.input :end_date %>
    </div>

   <div class="form-actions">
      <%= f.submit "Book !", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
